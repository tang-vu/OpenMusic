{
  "$schema": "https://tauri.app/schema/tauri.conf.json",
  "productName": "OpenMusic",
  "version": "0.1.0",
  "identifier": "com.openmusic.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": false,
    "targets": "all"
  },
  "app": {
    "windows": [
      {
        "title": "OpenMusic",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {}
}
