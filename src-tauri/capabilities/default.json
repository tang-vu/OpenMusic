{
  "$schema": "https://tauri.app/schema/capabilities.json",
  "identifier": "default",
  "description": "Default permissions for OpenMusic",
  "windows": ["main"],
  "permissions": [
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-exists",
    "shell:default",
    "shell:allow-execute",
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save"
  ]
}
